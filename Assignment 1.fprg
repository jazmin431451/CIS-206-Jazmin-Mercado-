    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="weight" type="Integer" array="False" size=""/>
            <declare name="height" type="Integer" array="False" size=""/>
            <declare name="bmi" type="Integer" array="False" size=""/>
            <output expression="&quot;Please Enter height in feet and inches&quot;" newline="True"/>
            <input variable="height"/>
            <output expression="&quot;also please enter your weight in lbs&quot;" newline="True"/>
            <input variable="weight"/>
            <assign variable="bmi" expression="weight/(height * 2) * 703"/>
            <if expression="bmi &lt;16">
                <then>
                    <output expression="&quot;severely underweight&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="bmi &gt;= 16 and bmi &lt; 18.5">
                        <then>
                            <output expression="&quot;underweight&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="bmi &gt;= 18.5 and bmi &lt; 25">
                                <then>
                                    <output expression="&quot;healthy&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="bmi &gt;= 25 and bmi &lt; 30">
                                        <then>
                                            <output expression="&quot;overweight&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="bmi &gt;= 30">
                                                <then>
                                                    <output expression="&quot;obese&quot;" newline="True"/>
                                                </then>
                                                <else/>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="&quot;your bmi is&quot; &amp;bmi &amp; &quot;and you are&quot; &amp;bmi" newline="True"/>
        </body>
    </function>
</flowgorithm>
